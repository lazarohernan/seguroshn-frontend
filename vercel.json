{
  "framework": "vite",
  "buildCommand": "vue-tsc -b && vite build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*).js",
      "dest": "/$1.js"
    },
    {
      "src": "/(.*).css",
      "dest": "/$1.css"
    },
    {
      "src": "/(.*).svg",
      "dest": "/$1.svg"
    },
    {
      "src": "/(.*).ico",
      "dest": "/$1.ico"
    },
    {
      "src": "/(.*).png",
      "dest": "/$1.png"
    },
    {
      "src": "/(.*).jpg",
      "dest": "/$1.jpg"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
